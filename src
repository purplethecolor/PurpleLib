					UICorner_3.Parent = Toggle3
					
					ToggleFS = false
					
					Toggle.MouseButton1Down:Connect(function()
						CircleAnim(Toggle,Color3.fromRGB(255,255,255),Color3.fromRGB(255,255,255))
						if ToggleFS == false then
							Toggle3:TweenSizeAndPosition(UDim2.new(0, 16, 0, 16), UDim2.new(0.5, 0, 0.5, 0), "In", "Quad", 0.1, true)
							wait(0.1)
							Toggle3:TweenSizeAndPosition(UDim2.new(0, 15, 0, 15), UDim2.new(0.5, 0, 0.5, 0), "In", "Quad", 0.1, true)
						else
							Toggle3:TweenSizeAndPosition(UDim2.new(0, 16, 0, 16), UDim2.new(0.5, 0, 0.5, 0), "In", "Quad", 0.1, true)
							wait(0.1)
							Toggle3:TweenSizeAndPosition(UDim2.new(0, 0, 0, 0), UDim2.new(0.5, 0, 0.5, 0), "In", "Quad", 0.1, true)
						end
						ToggleFS = not ToggleFS
						callback(ToggleFS)
					end)
					
					Toggle2.MouseButton1Down:Connect(function()
						CircleAnim(Toggle,Color3.fromRGB(255,255,255),Color3.fromRGB(255,255,255))
						if ToggleFS == false then
							Toggle3:TweenSizeAndPosition(UDim2.new(0, 16, 0, 16), UDim2.new(0.5, 0, 0.5, 0), "In", "Quad", 0.1, true)
							wait(0.1)
							Toggle3:TweenSizeAndPosition(UDim2.new(0, 15, 0, 15), UDim2.new(0.5, 0, 0.5, 0), "In", "Quad", 0.1, true)
						else
							Toggle3:TweenSizeAndPosition(UDim2.new(0, 16, 0, 16), UDim2.new(0.5, 0, 0.5, 0), "In", "Quad", 0.1, true)
							wait(0.1)
							Toggle3:TweenSizeAndPosition(UDim2.new(0, 0, 0, 0), UDim2.new(0.5, 0, 0.5, 0), "In", "Quad", 0.1, true)
						end
						ToggleFS = not ToggleFS
						callback(ToggleFS)
					end)
					
					Toggle3.MouseButton1Down:Connect(function()
						CircleAnim(Toggle,Color3.fromRGB(255,255,255),Color3.fromRGB(255,255,255))
						if ToggleFS == false then
							Toggle3:TweenSizeAndPosition(UDim2.new(0, 16, 0, 16), UDim2.new(0.5, 0, 0.5, 0), "In", "Quad", 0.1, true)
							wait(0.1)
							Toggle3:TweenSizeAndPosition(UDim2.new(0, 15, 0, 15), UDim2.new(0.5, 0, 0.5, 0), "In", "Quad", 0.1, true)
						else
							Toggle3:TweenSizeAndPosition(UDim2.new(0, 16, 0, 16), UDim2.new(0.5, 0, 0.5, 0), "In", "Quad", 0.1, true)
							wait(0.1)
							Toggle3:TweenSizeAndPosition(UDim2.new(0, 0, 0, 0), UDim2.new(0.5, 0, 0.5, 0), "In", "Quad", 0.1, true)
						end
						ToggleFS = not ToggleFS
						callback(ToggleFS)
					end)
				end
				
