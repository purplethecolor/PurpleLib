				Button.ClipsDescendants = true
				Button.BackgroundColor3 = Color3.fromRGB(160, 64, 255)
				Button.BackgroundTransparency = 0.500
				Button.BorderSizePixel = 0
				Button.Size = UDim2.new(0, 356, 0, 21)
